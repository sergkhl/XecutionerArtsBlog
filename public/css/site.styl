.reveal-animation
    // add the vendor prefixed versions as well
    transition: all 1s;
    -webkit-filter: grayscale(100%);

.reveal-animation:hover
  //opacity: 0.5;
  -webkit-filter: grayscale(0%);

.glow-animation
  text-shadow: 0 0 0px #ffffff
  transition: all 0.5s;

.glow-animation:hover
  text-shadow: 0 0 10px #f9ff4c

.navbar, navbar-header {

  background-color: #2c2c2c; /* fallback color, place your own */

  /* Gradients for modern browsers, replace as you see fit */
  background-image: -moz-linear-gradient(top, #5e5e5e, #818181);
  background-image: -ms-linear-gradient(top, #5e5e5e, #818181);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#5e5e5e), to(#818181));
  background-image: -webkit-linear-gradient(top, #5e5e5e, #818181);
  background-image: -o-linear-gradient(top, #5e5e5e, #818181);
  background-image: linear-gradient(top, #5e5e5e, #818181);
  background-repeat: repeat-x;
}

.three-columns
  -webkit-column-count:3; /* Chrome, Safari, Opera */
  -moz-column-count:3; /* Firefox */
  column-count:3;

.css-class-add, .css-class-remove
    -webkit-transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;
    -moz-transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;
    -o-transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;
    transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;

.css-class,
.css-class-add.css-class-add-active
    color: red;
    font-size:3em;

.css-class-remove.css-class-remove-active
    font-size:1.0em;
    color:black;



@keyframes animateMe-add
  from {opacity:0;}
  to {opacity:1;}

@-webkit-keyframes animateMe-add
  from {opacity:0;}
  to {opacity:1;}

@keyframes animateMe-remove
  from {opacity: 1;}
  to {opacity: 0;}

@-webkit-keyframes animateMe-remove
  from {opacity: 1;}
  to {opacity: 0;}

.fade-in.ng-enter
  -webkit-animation: 2s fade-in-enter-animation; animation: 2s fade-in-enter-animation;

.fade-in.ng-leave
  -webkit-animation: 2s fade-in-leave-animation; animation: 2s fade-in-leave-animation;
